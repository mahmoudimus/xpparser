<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://www.w3.org/2005/XQueryX"
            targetNamespace="http://www.w3.org/2005/XQueryX"
            elementFormDefault="qualified"
            attributeFormDefault="qualified">

  <xsd:simpleType name="boolFunction">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="true"/>
      <xsd:enumeration value="false"/>
      <xsd:enumeration value="boolean"/>
      <xsd:enumeration value="not"/>
      <xsd:enumeration value="boolean-equal"/>
      <xsd:enumeration value="boolean-less-than"/>
      <xsd:enumeration value="boolean-greater-than"/>
    </xsd:restriction>
  </xsd:simpleType>
  
</xsd:schema>
